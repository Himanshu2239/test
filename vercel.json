{
  "version": 2,
  "builds": [
    {
      "src": "build-vercel.sh",
      "use": "@vercel/bash"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
